<UserControl x:Class="AIM.Annotation.View.WinForms.Wpf.AnnotationNameWpfControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    DataContext="{Binding RelativeSource={RelativeSource Self}}">
    <Grid>
        <Label HorizontalAlignment="Left" Margin="0,0,0,0" Name="_lblAnnotationName" VerticalAlignment="Top">Annotation Name:</Label>
        <TextBox HorizontalAlignment="Left" Margin="4,24,0,0" Name="_txtAnnotationName" VerticalAlignment="Top" Text="{Binding Component.AnnotationName}"/>
    </Grid>
</UserControl>
